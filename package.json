    "request": "^2.87.0",
    "request-promise": "^4.2.2",
    "ts-node": "^6.1.0"
  },
  "devDependencies": {
    "@types/request-promise": "^4.1.41",
    "chai": "^4.1.2",
    "eslint": "^4.18.1",
    "eslint-config-standard": "10.2.1",
    "eslint-plugin-import": "^2.9.0",
